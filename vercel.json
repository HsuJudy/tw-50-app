{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/launch.html",
      "dest": "/launch.html"
    }
  ],

  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ]
}